{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log- .txt",
          "rollingInternal": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": {"serverUrl" :  "https://localhost:5241"}
      }
    ]
  },
  "AllowedHosts": "*"
}
